
@if (TempData["Warning"] != null)
{
    <p>&nbsp;</p>

    <div class="alert alert-warning alert-dismissible" role="alert">
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        @TempData["Warning"]
    </div>
}
